{
  "name": "frontend-template-hosted-qianxueseng",
  "version": "1.0.0",
  "description": "A sample app hosted by LeanCloud",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "babel": "./node_modules/.bin/babel --plugins transform-react-jsx public/js/main.js  > public/js/bundle.js",
    "browserify": "./node_modules/.bin/browserify public/js/bundle.js  > public/js/bundle-final.js",
    "build": "npm run babel && npm run browserify"
  },
  "keywords": [
    "node",
    "LeanCloud",
    "LeanEngine",
    "express",
    "qianxueseng"
  ],
  "license": "MIT",
  "dependencies": {
    "express": "4.12.3",
    "leanengine": "^1.0.0-beta",
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "engines": {
    "node": "4.x"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "browserify": "^13.0.1"
  }
}
